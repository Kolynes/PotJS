      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Kolynes/potjs.git"
   },
   "keywords": [
      "web",
      "typescript",
      "framework"
   ],
