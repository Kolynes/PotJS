   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Kolynes/PotJS/issues"
   },
   "homepage": "https://github.com/Kolynes/PotJS#readme",
   "devDependencies": {
      "@types/jsonwebtoken": "^8.5.6",
      "@types/node": "^8.0.29",
      "@types/validator": "^13.7.0",
      "nodemon": "^2.0.15",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/formidable": "^2.0.0",
      "formidable": "^2.0.1",
      "jsonwebtoken": "^8.5.1",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.41"
