{
   "name": "@kolynes/potjs",
   "version": "0.2.0",
   "description": "A simple typescript web framework ",
   "main": "index.js",
   "types": "index.d.ts",
   "scripts": {
      "prepublish": "tsc",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "start:demon": "nodemon ./src/index.ts",
      "db:init": "typeorm init",
      "test": "echo \"Error: no test specified\" && exit 1",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Kolynes/potjs.git"
   },
   "keywords": [
      "web",
      "typescript",
      "framework"
   ],
   "author": "Kolynes C. Chinedu",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Kolynes/PotJS/issues"
   },
   "homepage": "https://github.com/Kolynes/PotJS#readme",
   "devDependencies": {
      "@types/jsonwebtoken": "^8.5.6",
      "@types/mmmagic": "^0.4.30",
      "@types/node": "^8.0.29",
      "@types/nodemailer": "^6.4.4",
      "@types/promise-fs": "^2.1.2",
      "@types/validator": "^13.7.0",
      "nodemon": "^2.0.15",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@ssttevee/multipart-parser": "^0.1.9",
      "@types/formidable": "^2.0.0",
      "@types/parse-multipart": "^1.0.0",
      "formidable": "^2.0.1",
      "jsonwebtoken": "^8.5.1",
      "mmmagic": "^0.5.3",
      "nodemailer": "^6.7.2",
      "parse-multipart": "^1.0.4",
      "promise-fs": "^2.1.1",
