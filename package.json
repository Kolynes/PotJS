{
   "name": "@kolynes/potjs",
   "version": "0.1.1",
   "description": "A simple typescript web framework ",
   "main": "index.js",
   "types": "index.d.ts",
   "scripts": {
      "prepublish": "tsc",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "start:demon": "nodemon ./src/index.ts",
